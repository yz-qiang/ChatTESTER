<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="A1" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d [%-5p] [%X] [%c][%M][%L] - %m%n</pattern>
        </encoder>
    </appender>
    <logger name="org.apache" level="INFO"/>
    <root level="DEBUG">
        <appender-ref ref="A1"/>
    </root>
</configuration>
